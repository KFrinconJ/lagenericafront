<%- include("../../components/head-navbar", {tituloWeb: 'Clientes'}) %>

<div class="p-3 d-flex justify-content-center text-center">
    <h1 class="text-primary">Consulta de Cliente</h1>
  </div>
  <div class="container-fluid justify-content-center text-center">
    <form class="p-3 bg-light">
      <div class="form-row">
        <div class="form-group col-md-6"><div class="col px-md-8">
          <label for="Nombre">Nombre Completo</label>
          <input type="text" class="form-control" name="nombreCompleto" placeholder="Nombre Completo" disabled>
          <label for="Cédula">Cédula</label>
          <input type="number" class="form-control" name="cedula" placeholder="Número de Cédula" disabled>
          <label for="Dirección">Dirección</label>
          <input type="Dirección" class="form-control" name="direccion" placeholder="Direccion de residencia" disabled>
        </div></div>
        <div class="form-group col-md-6"><div class="col px-md-8">
          <label for="Teléfono">Teléfono</label>
          <input type="tel" class="form-control" name="telefono" placeholder="Número de teléfono" disabled>
          <label for="Correo">Correo Electrónico</label>
          <input type="email" class="form-control" name="correo" placeholder="Direccion de correo Electrónico" disabled>
        </div></div>
      </div>
      <div class="p-5 btn-group btn-group-toggle" data-toggle="buttons">
        <a class="btn btn-outline-dark" href="./" role="button">Regresar</a>
      </div>
    </form>
  </div>

  <!-- Alerta -->
	<% if(typeof(alerta)!="undefined" && alerta.length> 0 ) { %>

		<div class="d-flex justify-content-center text-center">
			<div class="container-fluid">
				<div class="alert alert-<%= colorAlerta %>"><%=alerta%></div>
				<% } %>

	<!-- Alerta -->


<%- include("../../components/footer") %>